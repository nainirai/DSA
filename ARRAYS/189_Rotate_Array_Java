import java.util.Stack;

class Solution {
    public void rotate(int[] nums, int k) {
        int n = nums.length;
        k=k%n;
        Stack<Integer> st = new Stack<Integer>();
        for(int i=n-1;i>n-k-1;i--){
                st.push(nums[i]);
        }
        for(int i=n-k-1;i>=0;i--){
                nums[i+k]=nums[i];
        } 
        for(int j=0;j<k;j++){
            nums[j]=st.pop();
            }
